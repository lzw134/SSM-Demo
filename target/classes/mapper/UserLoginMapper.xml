<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.system.dao.UserLoginDao" >
    <!--User getUserByUserName(String UserName);-->
    <select id="getUserByUserName" resultType="UserLogin">
        select * from store_user where username = '${username}'
    </select>

    <!--User getUserByUserName(String UserName);-->
    <select id="getAllUser" resultType="UserLogin">
        select * from store_user
    </select>
</mapper>
